{
	"name": "SNCR Plugins",
	"description": "A plugin of Sketch actions which are useful in the workflows of the designers at Synchronoss Technologies Inc.",
	"author": "Jason Burns",
	"homepage": "https://github.com/sonburn/sncr-sketch-plugin",
	"version": 0.44,
	"identifier": "com.sncr.sketch",
	"compatibleVersion": 3,
	"bundleVersion": 1,
	"commands": [
		{
			"name": "Toggle Toolbar",
			"shortcut": "cmd option l",
			"identifier": "layout-toolbar",
			"handler": "layoutBar",
			"script": "layout-toolbar.js"
		},
		{
			"name": "Layout Artboards…",
			"identifier": "layout-artboards",
			"script": "layout-artboards.js"
		},
		{
			"name": "Create Artboard Titles…",
			"identifier": "create-artboard-titles",
			"script": "create-artboard-titles.js"
		},
		{
			"name": "Create Artboard Slice…",
			"identifier": "create-artboard-slice",
			"script": "create-artboard-slice.js"
		},
		{
			"name": "Export Wireframes…",
			"identifier": "export-wireframes",
			"script": "export-wireframes.js"
		},
		{
			"name": "Find All Instances of Selected Symbol",
			"identifier": "find-symbol-instances",
			"script": "find-symbol-instances.js"
		}
	],
	"menu": {
		"isRoot": true,
		"items": [
			{
				"title": "SNCR - Layout & Wireframes",
				"items": [
					"layout-toolbar",
					"-",
					"layout-artboards",
					"create-artboard-titles",
					"create-artboard-slice",
					"export-wireframes"
				]
			},
			{
				"title": "SNCR - Symbol Tools",
				"items": [
					"find-symbol-instances"
				]
			}
		]
	}
}
